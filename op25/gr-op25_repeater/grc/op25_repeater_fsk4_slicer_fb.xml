<?xml version="1.0"?>
<block>
  <name>FSK4 Slicer</name>
  <key>op25_repeater_fsk4_slicer_fb</key>
  <category>op25_repeater</category>
  <import>import op25_repeater</import>
  <make>op25_repeater.fsk4_slicer_fb($slice_levels)</make>
  <param>
    <name>Slicer Levels</name>
    <key>slice_levels</key>
    <value>[-2.0, 0.0, 2.0, 4.0]</value>
    <type>float_vector</type>
  </param>
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
